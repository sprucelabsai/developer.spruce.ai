{
  "version": "5.6.0",
  "name": "developer.spruce.ai",
  "description": "Spruce Experience Platform Documentation. https://developer.spruce.ai",
  "skill": {
    "upgradeIgnoreList": [
      "@sprucelabs/resolve-path-aliases",
      "@types/node",
      "chokidar-cli",
      "concurrently",
      "prettier",
      "ts-node",
      "tsc-watch",
      "typescript"
    ]
  },
  "scripts": {
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "docs": "docsify serve -p 3100 ./",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "update.dependencies": "yarn clean.dependencies && yarn",
    "build.ci": "true",
    "build.dev": "true",
    "build.copy-files": "true",
    "build.resolve-paths": "true",
    "build.tsc": "true",
    "fix.lint": "true",
    "lint": "true",
    "lint.tsc": "true",
    "post.watch.build": "true",
    "resolve-paths.lint": "true",
    "test": "true",
    "watch.build.dev": "true",
    "watch.lint": "true",
    "watch.rebuild": "true",
    "watch.tsc": "true"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@sprucelabs/semantic-release": "^4.0.8",
    "conventional-changelog-sprucelabs": "^1.1.2",
    "docsify": "^4.12.1",
    "docsify-cli": "^4.4.3",
    "eslint": "^8.4.1",
    "eslint-config-spruce": "^10.11.0",
    "semantic-release": "^18.0.1"
  }
}
